@model IEnumerable<WebsiteChuyenDe1.Models.AdminAccount>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

    <h2 style="text-align:center;padding-top:1em">Thêm account admin</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TaiKhoan)
        </th>
        <th>
           
            @Html.DisplayNameFor(model => model.MatKhau)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td> @Html.DisplayFor(modelItem => item.TaiKhoan)</td>

    <td>

        @Html.DisplayFor(modelItem => item.MatKhau)
    </td>

    <td>
        <a href="@Url.Action( "Create","AdminAccounts")"><img src="~/images/edit_add.png" /></a>&emsp;
        <a href="@Url.Action( "Edit","AdminAccounts" ,new { id = item.TaiKhoan })"><img src="~/images/settings.png" /></a>&emsp;
        <a href="@Url.Action( "Delete","AdminAccounts", new { id = item.TaiKhoan })"><img src="~/images/delete.png" /></a>

    </td>
</tr>
}

</table>
